%h1
  Listing feeds
%table
  %tr
    - @feeds.each do |feed|
      %td
        = feed.feed_title
      %td
        = link_to 'Show', feed
      %td
        = link_to 'Edit', edit_feed_path(feed)
      %td
        = link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete

= link_to 'New feed', new_feed_path
